<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shoukailiang.community.system.mapper.SysFocusMapper">


    <select id="selectFans" resultType="java.lang.String">
        select user_id from sys_focus where focus_id=#{id}
    </select>

    <select id="selectFocus" resultType="java.lang.String">
        select focus_id from sys_focus where user_id=#{id}
    </select>
</mapper>
